{
  "printWidth": 160,
  "semi": true,
  "tabWidth": 2,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "css",
  "embeddedLanguageFormatting": "auto",
  "proseWrap": "preserve",
  "jsxBracketSameLine": false,
  "bracketSameLine": false,
  "quoteProps": "as-needed",
  "singleAttributePerLine": true,
  "vueIndentScriptAndStyle": false
}
